<li id="search{{ inputs.searchId }}">
  <div class="input-and-rates-container">
    <ul class="results-input">
      <li>
        <span>Street:</span>
        {{ inputs.street }}
      </li>
      <li>
        <span>City:</span>
        {{ inputs.city }}
      </li>
      <li>
        <span>State:</span>
        {{ inputs.state }}
      </li> 
      <li>
        <span>Zip:</span>
        {{ inputs.zip }}
      </li>
    </ul>
    <div class="no-rates-message">
      <p>We are unable to determine the tax rates for this location. Please 
        check the address and try again.</p>
    </div>
  </div>
  <div class="map-container" aria-label="google map with pin on the sales location">
    <div class="map"> </div>
  </div>
</li>
